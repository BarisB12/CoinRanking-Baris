<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/coin_nav"
    app:startDestination="@id/coinsFragment">

    <fragment
        android:id="@+id/coinsFragment"
        android:name="com.example.coinranking_baris.coins.CoinsFragment"
        android:label="fragment_coin"
        tools:layout="@layout/fragment_coin" >
        <action
            android:id="@+id/action_coinsFragment_to_detailCoinsFragment"
            app:destination="@id/detailCoinsFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailCoinsFragment"
        android:name="com.example.coinranking_baris.detailCoins.DetailCoinsFragment"
        android:label="fragment_detail_coins"
        tools:layout="@layout/fragment_detail_coins" />
</navigation>